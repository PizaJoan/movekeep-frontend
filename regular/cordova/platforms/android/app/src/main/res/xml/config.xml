<?xml version='1.0' encoding='utf-8'?>
<widget id="com.movekeep.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Movekeep</name>
    <description>
        Application for everyone to get in shape
    </description>
    <author email="pizajoan97@gmail.com" href="http://www.github.com/pizajoan">
        Joan Piza Ferra
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="hdpi" src="./www/img/logo-movekeep-working-png-big.fda7c3d.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
